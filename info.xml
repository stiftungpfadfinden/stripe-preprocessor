<?xml version="1.0"?>
<extension key="meinestiftung" type="module">
  <file>meinestiftung</file>
  <name>Stiftung Pfadfinden: Transform Stripe payment data</name>
  <description>Uses automatic Stripe payment intent and sends more sensible payment data and descriptions</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Andy Lietz</author>
    <email>andy.lietz@stiftungpfadfinden.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-30</releaseDate>
  <version>0.6</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.78</ver>
  </compatibility>
  <comments>Stable</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Meinestiftung</namespace>
    <format>24.09.1</format>
    <angularModule>crmMeinestiftung</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
