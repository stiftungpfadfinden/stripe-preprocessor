<?xml version="1.0"?>
<extension key="de.stiftungpfadfinden.stripe-preprocessor" type="module">
  <file>stripe_preprocessor</file>
  <name>Stripe Preprocessor</name>
  <description>Pre-process contribution data before it is sent to Stripe</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>Andy Lietz</name>
      <email>andy.lietz@stiftungpfadfinden.de</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://github.com/stiftungpfadfinden/stripe-preprocessor</url>
    <url desc="Documentation">https://github.com/stiftungpfadfinden/stripe-preprocessor</url>
    <url desc="Support">https://stiftungpfadfinden.de</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-04-21</releaseDate>
  <version>1.0.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.74</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/StripePreprocessor</namespace>
    <format>25.01.1</format>
    <angularModule>crmStripePreprocessor</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
